{
  "name": "mjml-converter",
  "version": "0.0.0",
  "description": "A simple server that converts mjml to html.",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint --ext .js,.mjs,.ts src/ && prettier --list-different \"**/*.{ts,js,mjs,json,md}\"",
    "lint-fix": "eslint --ext .js,.mjs,.ts --fix src/ && prettier --write \"**/*.{ts,js,mjs,json,md}\"",
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "test": "jest"
  },
  "keywords": [
    "mjml"
  ],
  "author": "Jonas Michaelis",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.20.2",
    "express": "4.18.2",
    "html-minifier": "4.0.0",
    "mjml": "4.10.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/html-minifier": "4.0.2",
    "@types/jest": "29.5.3",
    "@types/mjml": "4.7.0",
    "@types/morgan": "1.9.4",
    "@types/node": "18.17.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.6.2",
    "nodemon": "3.0.1",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "rimraf": "5.0.1",
    "supertest": "6.2.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
