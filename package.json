{
  "name": "mjml-converter",
  "version": "0.0.0",
  "description": "A simple server that converts mjml to html.",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint --ext .js,.mjs,.ts src/ && prettier --list-different \"**/*.{ts,js,mjs,json,md}\"",
    "lint-fix": "eslint --ext .js,.mjs,.ts --fix src/ && prettier --write \"**/*.{ts,js,mjs,json,md}\"",
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "test": "jest"
  },
  "keywords": [
    "mjml"
  ],
  "author": "Jonas Michaelis",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "html-minifier": "4.0.0",
    "mjml": "4.10.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/html-minifier": "4.0.1",
    "@types/jest": "26.0.24",
    "@types/mjml": "4.7.0",
    "@types/morgan": "1.9.3",
    "@types/node": "15.12.4",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.0.6",
    "nodemon": "2.0.12",
    "prettier": "2.3.2",
    "prettier-eslint": "13.0.0",
    "rimraf": "3.0.2",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5"
  }
}
