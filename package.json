{
  "name": "mjml-converter",
  "version": "0.0.0",
  "description": "A simple server that converts mjml to html.",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint --ext .js,.mjs,.ts src/ && prettier --list-different \"**/*.{ts,js,mjs,json,md}\"",
    "lint-fix": "eslint --ext .js,.mjs,.ts --fix src/ && prettier --write \"**/*.{ts,js,mjs,json,md}\"",
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "test": "jest"
  },
  "keywords": [
    "mjml"
  ],
  "author": "Jonas Michaelis",
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.2",
    "express": "4.17.3",
    "html-minifier": "4.0.0",
    "mjml": "4.10.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/html-minifier": "4.0.2",
    "@types/jest": "27.4.1",
    "@types/mjml": "4.7.0",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "prettier": "2.6.0",
    "prettier-eslint": "13.0.0",
    "rimraf": "3.0.2",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
